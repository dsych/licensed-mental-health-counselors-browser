{
  "name": "licensure-browser",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf .data .cache build dist",
    "lint:typescript": "tsc --noEmit",
    "dev": "nodemon server/index.ts",
    "build:server": "tsc",
    "build:db": "mkdir .data && touch .data/db.json",
    "start": "npm run lint:typescript && npm run clean && npm run build:db && npm run build:server && node build/index.js"
  },
  "dependencies": {
    "axios": "latest",
    "body-parser": "^1.18.3",
    "date-fns": "^1.29.0",
    "elasticsearch": "^15.1.1",
    "express": "^4.16.3",
    "ftp": "^0.3.10",
    "fuzzysort": "^1.1.4",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "parcel-bundler": "^1.10.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-virtualized": "^9.20.1",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/elasticsearch": "^5.0.26",
    "@types/express": "^4.16.0",
    "@types/ftp": "^0.3.29",
    "@types/lowdb": "^1.0.5",
    "@types/react": "^16.4.15",
    "@types/react-dom": "^16.0.8",
    "@types/react-virtualized": "^9.18.7",
    "dotenv-cli": "^1.4.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  },
  "keywords": [],
  "nodemonConfig": {
    "ignore": [
      "src/*",
      "dist/*",
      ".cache/*",
      ".data/*"
    ],
    "exec": "ts-node"
  },
  "engines": {
    "node": "10.x"
  }
}
